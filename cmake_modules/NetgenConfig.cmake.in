# - Config file for the NETGEN package
# It defines the following variables
#  NETGEN_NGLIB_INCLUDE_PATH - include directory for NETGEN

get_filename_component(SELF_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

set(PACKAGE_VERSION "@NETGEN_VERSION@")

set(NETGEN_NGLIB_INCLUDE_PATH "${SELF_DIR}/../include/netgen")
